{
  "headers": [
    {
      "source": "/(.*)\\.js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    },
    {
      "source": "/(.*)\\.pdf",
      "headers": [
        {
          "key": "Content-Type", 
          "value": "application/pdf"
        }
      ]
    }
  ],
  "trailingSlash": false,
  "routes": [
    { 
      "handle": "filesystem" 
    }
  ]
}
